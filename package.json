{
	"name": "pg-tailwind3-app",
	"version": "1.0.0",
	"description": "Pinegrow external build with tailwind plugin",
	"scripts": {
		"dev": "npm-run-all --parallel watch uno-watch",
		"watch": "npx tailwindcss -i src/styles/tailwind.css  -o dist/assets/css/output.css  --watch",
		"uno-watch": "unocss --watch"
	},
	"devDependencies": {
		"@iconify/json": "^2.1.152",
		"@iconify/tools": "^2.1.2",
		"@pinegrow/tailwind-plugin": "latest",
		"@tailwindcss/typography": "^0.5.4",
		"@unocss/cli": "^0.47.6",
		"@unocss/preset-icons": "^0.47.6",
		"autoprefixer": "^10.4.8",
		"daisyui": "^2.43.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.16",
		"tailwindcss": "^3.2.4",
		"unocss": "^0.47.6"
	},
	"dependencies": {
		"iconify-icon": "^1.0.2"
	}
}
